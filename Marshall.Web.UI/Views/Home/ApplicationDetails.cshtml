@model Marshall.Web.UI.Models.Version.ApplicationViewModel

<fieldset>
    <legend>Version</legend>

    <table>
        <tr>
            <td style="font-style:italic">Name</td>
            <td> @Html.DisplayFor(model => model.Name)</td>
        </tr>
        <tr>
            <td style="font-style:italic">Type</td>
            <td> @Html.DisplayFor(model => model.Type)</td>
        </tr>
         <tr>
            <td style="font-style:italic">Product Code</td>
            <td> @Html.DisplayFor(model => model.ProductCode)</td>
        </tr>
         <tr>
            <td style="font-style:italic">TeamCity Project Name</td>
            <td> @Html.DisplayFor(model => model.TeamCityProjectName)</td>
        </tr>
         <tr>
            <td style="font-style:italic">Build Name</td>
            <td> @Html.DisplayFor(model => model.TeamCityBuildName)</td>
        </tr>
         <tr>
            <td style="font-style:italic">Specified Version</td>
            <td> @Html.DisplayFor(model => model.SpecifiedVersion)</td>
        </tr>
         <tr>
            <td style="font-style:italic">Installation File</td>
            <td> @Html.DisplayFor(model => model.FileToInstall)</td>
        </tr>
         <tr>
            <td style="font-style:italic">Config File</td>
            <td> @Html.DisplayFor(model => model.ConfigFile)</td>
        </tr>
         <tr>
            <td style="font-style:italic">.NET Framework Version</td>
            <td> @Html.DisplayFor(model => model.NetFrameworkVersion)</td>
        </tr>
         <tr>
            <td style="font-style:italic">Working Directory Prefix</td>
            <td> @Html.DisplayFor(model => model.WorkingDirectoryPrefix)</td>
        </tr>
         <tr>
            <td style="font-style:italic">SVN Prefix</td>
            <td> @Html.DisplayFor(model => model.SubversionPrefix)</td>
        </tr>
         <tr>
            <td style="font-style:italic">SVN Working Directory Prefix</td>
            <td> @Html.DisplayFor(model => model.SubversionWorkingDirectoryPrefix)</td>
        </tr>
         <tr>
            <td style="font-style:italic">SVN Specified Version</td>
            <td> @Html.DisplayFor(model => model.SubversionSpecifiedVersion)</td>
        </tr>
    </table>
</fieldset>
