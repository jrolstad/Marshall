@model Marshall.Web.UI.Models.Version.ApplicationViewModel

<fieldset>
    <legend>Version</legend>

    <table>
        <tr>
            <td style="text-decoration:underline">Name</td>
            <td> @Html.DisplayFor(model => model.Name)</td>
        </tr>
        <tr>
            <td style="text-decoration:underline">Type</td>
            <td> @Html.DisplayFor(model => model.Type)</td>
        </tr>
         <tr>
            <td style="text-decoration:underline">Product Code</td>
            <td> @Html.DisplayFor(model => model.ProductCode)</td>
        </tr>
         <tr>
            <td style="text-decoration:underline">TeamCity Project Name</td>
            <td> @Html.DisplayFor(model => model.TeamCityProjectName)</td>
        </tr>
         <tr>
            <td style="text-decoration:underline">Build Name</td>
            <td> @Html.DisplayFor(model => model.TeamCityBuildName)</td>
        </tr>
         <tr>
            <td style="text-decoration:underline">Specified Version</td>
            <td> @Html.DisplayFor(model => model.SpecifiedVersion)</td>
        </tr>
         <tr>
            <td style="text-decoration:underline">Installation File</td>
            <td> @Html.DisplayFor(model => model.FileToInstall)</td>
        </tr>
         <tr>
            <td style="text-decoration:underline">Config File</td>
            <td> @Html.DisplayFor(model => model.ConfigFile)</td>
        </tr>
         <tr>
            <td style="text-decoration:underline">.NET Framework Version</td>
            <td> @Html.DisplayFor(model => model.NetFrameworkVersion)</td>
        </tr>
         <tr>
            <td style="text-decoration:underline">Working Directory Prefix</td>
            <td> @Html.DisplayFor(model => model.WorkingDirectoryPrefix)</td>
        </tr>
         <tr>
            <td style="text-decoration:underline">SVN Prefix</td>
            <td> @Html.DisplayFor(model => model.SubversionPrefix)</td>
        </tr>
         <tr>
            <td style="text-decoration:underline">SVN Working Directory Prefix</td>
            <td> @Html.DisplayFor(model => model.SubversionWorkingDirectoryPrefix)</td>
        </tr>
         <tr>
            <td style="text-decoration:underline">SVN Specified Version</td>
            <td> @Html.DisplayFor(model => model.SubversionSpecifiedVersion)</td>
        </tr>
    </table>
</fieldset>
